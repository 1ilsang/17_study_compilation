package kotilnTest;

public class fan {
	private static int ON = 0;
	private static int OFF = 1;
	private static int WORKING = 2;
	private int power_exist;
	private int state;
	
	public fan(int power_exist){
		state = OFF;
		this.power_exist = power_exist;
	}
	
	public void switch_on(){
		if(power_exist){
			turnon();
		}
	}
}
