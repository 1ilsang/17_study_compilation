package kotilnTest;

public class Light {
	private State state;
	public Light(){
		state = new OFF(); //초기 형광등은 꺼져있다.
	}
	public void on_button_pushed(){
		state
	}
	public void off_button_pushed() {
		if(state == OFF){
			System.out.println("반응 없음");
		}else{
			System.out.println("Light Off!");
			state = OFF;
		}
	}
}

