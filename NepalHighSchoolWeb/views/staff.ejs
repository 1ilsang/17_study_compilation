<style type="text/css">
         blockquote{
             background: #f9f9f9;
             border-left: 10px solid #cccccc;
             margin: 1.5em 10px;
             padding: 0.5em 10px;
             quotes: "\201C""\201D""\2018""\2019";
         }
         blockquote:before{
             color: #cccccc;
             content: open-quote;
             font-size: 3em;
             line-height: 0.1em;
             margin-left: 0.25em;
             vertical-align: -0.4em;
         }
         blockquote:after{
             color: #cccccc;
             content: close-quote;
             font-size: 3em;
             line-height: 0.1em;
             margin-left: 0.25em;
             vertical-align: -0.4em;
         }
         @import url(http://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,700,400italic);

        font-family: 'Open Sans', sans-serif;

